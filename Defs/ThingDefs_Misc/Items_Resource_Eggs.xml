  <!--=====================================================================-->
  <!--=============================== Eggs ================================-->
  <!--=====================================================================-->

  <ThingDef Abstract="True" Name="EggBase" ParentName="OrganicProductBase" >
    <graphicData>
      <texPath>Things/Item/Resource/AnimalProductRaw/EggBirdSmall</texPath>
      <graphicClass>Graphic_StackCount</graphicClass>
    </graphicData>
    <ingestible>
      <foodType>AnimalProduct</foodType>
      <nutrition>0.25</nutrition>
      <ingestEffect>EatMeat</ingestEffect>
      <ingestSound>RawMeat_Eat</ingestSound>
      <tastesRaw>true</tastesRaw>
    </ingestible>
  </ThingDef>

  <ThingDef Abstract="True" Name="EggUnfertBase" ParentName="EggBase" >
    <ingestible>
      <preferability>RawBad</preferability>
    </ingestible>
    <thingCategories>
      <li>EggsUnfertilized</li>
    </thingCategories>
    <comps>
      <li Class="CompProperties_Rottable">
        <daysToRotStart>15</daysToRotStart>
      </li>
      <li Class="CompProperties_FoodPoisoningChance" />
    </comps>
  </ThingDef>

  <ThingDef Abstract="True" Name="EggFertBase" ParentName="EggBase" >
    <tickerType>Normal</tickerType>
    <ingestible>
      <preferability>DesperateOnly</preferability>
    </ingestible>
    <thingCategories>
      <li>EggsFertilized</li>
    </thingCategories>
  </ThingDef>

  <!-- Cobra -->
  
  <ThingDef ParentName="EggFertBase">
    <defName>EggKomodoFertilized</defName>
    <label>Komodo dragon egg (fert.)</label>
    <description>Fertilized Komodo dragon egg. If all goes well, it should hatch into a baby Komodo dragon. It can be eaten raw, but it's much better cooked.</description>
    <graphicData>
      <texPath>Things/Item/Resource/AnimalProductRaw/EggOval</texPath>
      <color>(245,245,245)</color>
    </graphicData>
    <statBases>
      <MarketValue>40.0</MarketValue>
    </statBases>
    <comps>
      <li Class="CompProperties_Hatcher">
        <hatcherDaysToHatch>6</hatcherDaysToHatch>
        <hatcherPawn>KomodoDragon</hatcherPawn>
      </li>
    </comps>
  </ThingDef>  
  
</Defs>