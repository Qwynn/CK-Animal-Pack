<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef ParentName="PlantFoodRawBase">
    <defName>CK_TaroRoot</defName>
    <label>taro roots</label>
    <description>Taro roots.</description>
    <graphicData>
      <texPath>Things/Item/Resource/PlantFoodRaw/TaroRoot</texPath>
    </graphicData>
    <statBases>
      <MarketValue>1.5</MarketValue>
    </statBases>
    <ingestible>
      <foodType>VegetableOrFruit</foodType>
    </ingestible>
    <comps>
      <li Class="CompProperties_Rottable">
        <daysToRotStart>30</daysToRotStart>
      </li>
    </comps>
  </ThingDef>

  <ThingDef ParentName="PlantFoodRawBase">
    <defName>CK_RawBanana</defName>
    <label>bananas</label>
    <description>Bananas. Nice to eat, even when raw.</description>
    <graphicData>
      <texPath>Things/Item/Resource/PlantFoodRaw/BananaFruit</texPath>
    </graphicData>
    <statBases>
      <MarketValue>1.5</MarketValue>
    </statBases>
    <comps>
      <li Class="CompProperties_Rottable">
        <daysToRotStart>14</daysToRotStart>
      </li>
    </comps>
    <ingestible>
      <preferability>RawTasty</preferability>
      <tastesRaw>false</tastesRaw>
      <joy>0.01</joy>
      <joyKind>Gluttonous</joyKind>
      <foodType>VegetableOrFruit</foodType>
    </ingestible>
  </ThingDef>

  <ThingDef ParentName="PlantFoodRawBase">
    <defName>CK_RawLemongrass</defName>
    <label>lemongrass</label>
    <description></description>
    <graphicData>
      <texPath>Things/Item/Resource/PlantFoodRaw/Lemongrass</texPath>
    </graphicData>
    <statBases>
      <MarketValue>0.9</MarketValue>
    </statBases>
    <comps>
      <li Class="CompProperties_Rottable">
        <daysToRotStart>20</daysToRotStart>
        <rotDestroys>true</rotDestroys>
      </li>
    </comps>
    <ingestible>      
      <foodType>VegetableOrFruit</foodType>
    </ingestible>    
  </ThingDef>

  <ThingDef ParentName="PlantFoodRawBase">
    <defName>CK_Coconuts</defName>
    <label>coconuts</label>
    <description>Coconuts. Nice to eat, even when raw.</description>
    <graphicData>
      <texPath>Things/Item/Resource/PlantFoodRaw/Coconuts</texPath>
    </graphicData>
    <statBases>
      <MarketValue>1</MarketValue>
    </statBases>
    <comps>
      <li Class="CompProperties_Rottable">
        <daysToRotStart>30</daysToRotStart>
      </li>
    </comps>
    <ingestible>
      <preferability>RawTasty</preferability>
      <tastesRaw>false</tastesRaw>
      <joy>0.01</joy>
      <joyKind>Gluttonous</joyKind>      
      <foodType>VegetableOrFruit</foodType>
    </ingestible>
  </ThingDef>

  <ThingDef ParentName="PlantFoodRawBase">
    <defName>CK_RawCoffee</defName>
    <label>coffee beans</label>
    <description>Can be cooked or can give a little boost to movement speed and "Rest need" if eaten raw, but taste bad. (You may want to check if you want this to be used in cooking or not)</description>
    <soundInteract>Grain_Drop</soundInteract>
    <soundDrop>Grain_Drop</soundDrop>
    <graphicData>
      <texPath>Things/Item/Resource/PlantFoodRaw/CoffeeBeans</texPath>
    </graphicData>
    <statBases>
      <MarketValue>4.5</MarketValue>
    </statBases>
    <ingestible>
      <maxNumToIngestAtOnce>4</maxNumToIngestAtOnce>
      <baseIngestTicks>120</baseIngestTicks>
      <joyKind>Chemical</joyKind>
      <joy>0.01</joy>
      <foodType>Seeds</foodType>
      <nutrition>0.01</nutrition>
      <outcomeDoers>        
        <li Class="IngestionOutcomeDoer_OffsetNeed">
          <need>Rest</need>
          <offset>0.1</offset>
        </li>
        <li Class="IngestionOutcomeDoer_GiveHediff">
          <hediffDef>CK_AteRawCoffee</hediffDef>          
        </li>
      </outcomeDoers>
    </ingestible>    
  </ThingDef>

  <HediffDef>
    <defName>CK_AteRawCoffee</defName>
    <label>Cafeine</label>
    <initialSeverity>1</initialSeverity>
    <hediffClass>HediffWithComps</hediffClass>
    <comps>
      <li>
        <compClass>HediffComp_Immunizable</compClass>
        <severityPerDayNotImmune>-12</severityPerDayNotImmune>
      </li>
    </comps>
    <stages>
        <li>
          <minSeverity>0</minSeverity>
          <label>speed from coffee</label>          
            <capMods>
              <li>
                <capacity>Moving</capacity>
                <offset>0.05</offset>
              </li>
            </capMods>
        </li>
      </stages>
  </HediffDef>

  <ThingDef ParentName="OrganicProductBase">
    <defName>CK_Honeycomb</defName>
    <label>honeycomb</label>
    <description>Honey stored and used as food by bees. It is smooth, rich, and brings joy to those who consume it. Because of its unique biological properties, it can nourish almost any creature and never rots.</description>
    <graphicData>
      <texPath>Things/Item/Resource/PlantFoodRaw/Honeycomb</texPath>
    </graphicData>
    <statBases>
      <MarketValue>7.0</MarketValue>
    </statBases>
    <ingestible>
      <foodType>AnimalProduct</foodType>
      <preferability>MealFine</preferability>
      <nutrition>0.05</nutrition>
      <joy>0.05</joy>
      <joyKind>Gluttonous</joyKind>
      <ingestEffect>EatVegetarian</ingestEffect>
      <ingestSound>Meal_Eat</ingestSound>
    </ingestible>
    <thingCategories>
      <li>AnimalProductRaw</li>
    </thingCategories>
  </ThingDef>

  <ThingDef ParentName="PlantFoodRawBase">
    <defName>CK_DreamVineRoot</defName>
    <label>dream vine root</label>
    <description>Can be cooked or eaten raw (You may want to check if you want this to be used in cooking or not). Dream Vine Roots are traditionally used to induce vivid lucid dreams during the initiation process of shamans to help them remember their dreams. </description>
    <graphicData>
      <texPath>Things/Item/Resource/PlantFoodRaw/DreamVineRoot</texPath>
    </graphicData>
    <statBases>
      <MarketValue>2</MarketValue>
    </statBases>
    <comps>
      <li Class="CompProperties_Rottable">
        <daysToRotStart>30</daysToRotStart>
        <rotDestroys>true</rotDestroys>
      </li>
      <li Class="CompProperties_Drug">
        <chemical>Alcohol</chemical>
        <addictiveness>0.003</addictiveness>
        <minToleranceToAddict>0.5</minToleranceToAddict>
        <existingAddictionSeverityOffset>0.10</existingAddictionSeverityOffset>
        <needLevelOffset>1</needLevelOffset>
        <listOrder>10</listOrder>
      </li>
    </comps>    
     <ingestible>
     <drugCategory>Social</drugCategory>
     <maxNumToIngestAtOnce>4</maxNumToIngestAtOnce>
     <baseIngestTicks>120</baseIngestTicks>
     <joyKind>Chemical</joyKind>
     <joy>0.1</joy>
     <foodType>VegetableOrFruit</foodType>
     <nutrition>0.01</nutrition>
      <outcomeDoers>
        <li Class="IngestionOutcomeDoer_GiveHediff">
          <hediffDef>AlcoholHigh</hediffDef>
          <severity>0.1</severity>
          <toleranceChemical>Alcohol</toleranceChemical>
        </li>
        <li Class="IngestionOutcomeDoer_GiveHediff">
          <hediffDef>AlcoholTolerance</hediffDef>
          <severity>0.01</severity>
        </li>
      </outcomeDoers>
    </ingestible>
  </ThingDef>
  
</Defs>